{
  "name": "sveltekit-pwa-template",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && npx svelte-package && npx publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && npx svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && npx svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && npx eslint .",
    "test:unit": "vitest",
    "test": "bun test:unit && bun test:e2e",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@playwright/test": "^1.45.3",
    "@storybook/addon-essentials": "^8.4.0",
    "@storybook/addon-interactions": "^8.4.0",
    "@storybook/addon-svelte-csf": "^5.0.0-next.10",
    "@storybook/blocks": "^8.4.0",
    "@storybook/svelte": "^8.4.0",
    "@storybook/sveltekit": "^8.4.0",
    "@storybook/test": "^8.4.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.4",
    "@types/eslint": "^9.6.0",
    "@types/web-push": "^3.6.4",
    "@vitest/coverage-v8": "2.1.4",
    "@vitest/ui": "2.1.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "jsdom": "^25.0.1",
    "msw": "^2.6.0",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "publint": "^0.2.0",
    "storybook": "^8.4.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.11",
    "vite-plugin-pwa": "^0.20.5",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "@vite-pwa/sveltekit": "^0.6.6",
    "web-push": "^3.6.7",
    "workbox-window": "^7.3.0"
  }
}
